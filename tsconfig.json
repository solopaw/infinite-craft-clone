{
  "compilerOptions": {
   
    "target": "es2016",
    "jsx": "preserve",
    "module": "commonjs",
      "lib": [
        "es2016",
        "dom"
      ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
